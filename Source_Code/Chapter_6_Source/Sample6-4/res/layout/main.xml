<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ImageView android:id="@+id/image_google_logo"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:layout_marginLeft="5.0dip" android:layout_marginRight="5.0dip" />

	<ImageView android:id="@+id/image_android_logo"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:layout_marginLeft="5.0dip" android:layout_marginRight="5.0dip"
		android:layout_marginBottom="5.0dip" android:layout_below="@id/image_google_logo" />

	<Button android:id="@+id/button_http_url" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_marginBottom="5.0dip"
		android:layout_marginLeft="5.0dip" android:layout_marginRight="5.0dip"
		android:text="@string/button_http_url"
		android:layout_alignParentBottom="true" />

	<Button android:id="@+id/button_url" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_marginBottom="5.0dip"
		android:layout_marginLeft="5.0dip" android:layout_marginRight="5.0dip"
		android:text="@string/button_url" android:layout_above="@id/button_http_url" />




</RelativeLayout>
