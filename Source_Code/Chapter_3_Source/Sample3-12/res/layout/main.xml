<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <Button android:id="@+id/dialog_alert1"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:text="@string/dialog_alert1"/>
    <Button android:id="@+id/dialog_alert2"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:text="@string/dialog_alert2"/>
    <Button android:id="@+id/dialog_progress"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:text="@string/dialog_progress"/>
    <Button android:id="@+id/dialog_date_picker"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:text="@string/dialog_date_picker"/>
    <Button android:id="@+id/dialog_time_picker"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:text="@string/dialog_time_picker"/>
</LinearLayout>
